<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">

<liferay-portlet-app>
        <portlet>
                <portlet-name>actorsForm</portlet-name>     
                <social-request-interpreter-class>se.gothiaforum.util.MembersRequestInterpreter</social-request-interpreter-class>
                <css-class-wrapper>gothia-actorsform-portlet</css-class-wrapper>                         
        </portlet>
        <portlet>
                <portlet-name>actorsSearch</portlet-name>   
                <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
                <friendly-url-mapping>sok</friendly-url-mapping>
                <friendly-url-routes>actors-search-friendly-url-routes.xml</friendly-url-routes>
                <css-class-wrapper>gothia-actorssearch-portlet</css-class-wrapper>    
        </portlet> 
        <portlet>
                <portlet-name>actorsSearchResult</portlet-name>   
                <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
                <friendly-url-mapping>aktor</friendly-url-mapping>
                <friendly-url-routes>actors-search-result-friendly-url-routes.xml</friendly-url-routes>
                <css-class-wrapper>gothia-actorssearchresult-portlet</css-class-wrapper>    
        </portlet> 
        <portlet>
                <portlet-name>actorsSearchThinClient</portlet-name>   
                <css-class-wrapper>gothia-searchactors-portlet</css-class-wrapper>    
        </portlet>     
        <portlet>
        <portlet-name>settings</portlet-name>   
                <control-panel-entry-category>content</control-panel-entry-category>
                <control-panel-entry-weight>7.5</control-panel-entry-weight>
                <css-class-wrapper>gothia-settings-portlet</css-class-wrapper>    
        </portlet>      
         <portlet>
                <portlet-name>moreLikeThis</portlet-name>   
                <css-class-wrapper>gothia-morelikethis-portlet</css-class-wrapper>    
        </portlet>               
</liferay-portlet-app>
