<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
  id="gothia_actors-core-bc-module-portlet">
  <portlet>
    <portlet-name>actorsForm</portlet-name>
    <display-name>Actors Form</display-name>
    <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
    </supports>
    <supported-locale>sv</supported-locale>
    <resource-bundle>i18n.actors-messages</resource-bundle>
    <portlet-info>
      <title>Actors Form</title>
      <short-title>Actors Form</short-title>
      <keywords></keywords>
    </portlet-info>
  </portlet>
  
  <portlet>
    <portlet-name>actorsSearch</portlet-name>
    <display-name>Actors Search</display-name>
    <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
      <portlet-mode>EDIT</portlet-mode>
    </supports>
    <supported-locale>sv</supported-locale>
    <resource-bundle>i18n.actors-messages</resource-bundle>
    <portlet-info>
      <title>Actors Search</title>
      <short-title>Actors Search</short-title>
      <keywords></keywords>
    </portlet-info>
    <supported-public-render-parameter>searchTerm</supported-public-render-parameter>
    <supported-public-render-parameter>friendlyURL</supported-public-render-parameter>
  </portlet>
  
  <portlet>
    <portlet-name>actorsSearchResult</portlet-name>
    <display-name>Actors Search Result</display-name>
    <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
    </supports>
    <supported-locale>sv</supported-locale>
    <resource-bundle>i18n.actors-messages</resource-bundle>
    <portlet-info>
      <title>Actors Search Result</title>
      <short-title>Actors Search Result</short-title>
      <keywords></keywords>
    </portlet-info>
    <supported-public-render-parameter>searchTerm</supported-public-render-parameter>
    <supported-public-render-parameter>friendlyURL</supported-public-render-parameter>
  </portlet>
  
  <portlet>
    <portlet-name>actorsSearchThinClient</portlet-name>
    <display-name>Actors Search Thin Client</display-name>
    <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
    </supports>   
    <supported-locale>sv</supported-locale>
    <resource-bundle>i18n.actors-messages</resource-bundle>
    <portlet-info>
      <title>Actors Search Thin Client</title>
      <short-title>Actors Search Thin</short-title>
      <keywords></keywords>
    </portlet-info>  
    <supported-public-render-parameter>searchTerm</supported-public-render-parameter>
  </portlet>
  
  <portlet>
    <portlet-name>settings</portlet-name>
    <display-name>Settings for Gothia Forum</display-name>
    <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
    </supports>   
    <supported-locale>sv</supported-locale>
    <resource-bundle>i18n.actors-messages</resource-bundle>
    <portlet-info>
      <title>Settings for Gothia</title>
      <short-title>settings</short-title>
      <keywords></keywords>
    </portlet-info>
  </portlet>
  
  <portlet>
    <portlet-name>moreLikeThis</portlet-name>
    <display-name>Show Similar Actors</display-name>
    <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
      <portlet-mode>EDIT</portlet-mode>
    </supports>   
    <supported-locale>sv</supported-locale>
    <resource-bundle>i18n.actors-messages</resource-bundle>
    <portlet-info>
      <title>Show Similar Actors</title>
      <short-title>Show Similar Actors</short-title>
      <keywords></keywords>
    </portlet-info>
    <supported-public-render-parameter>friendlyURL</supported-public-render-parameter>
  </portlet>
  
    <portlet>
    <portlet-name>tagCloud</portlet-name>
    <display-name>Gothia Actors Tag Cloud</display-name>
    <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
    </supports>   
    <supported-locale>sv</supported-locale>
    <resource-bundle>i18n.actors-messages</resource-bundle>
    <portlet-info>
      <title>Gothia Actors Tag Cloud</title>
      <short-title>Tag Cloud</short-title>
      <keywords></keywords>
    </portlet-info>
  </portlet>
  

   <!-- Used to get remote user. See more info in JSR-286 Section 21.1. -->
  <user-attribute>
    <description>User Login Id</description>
    <name>user.login.id</name>
  </user-attribute>  
  
   <public-render-parameter>
       <identifier>searchTerm</identifier>
       <qname xmlns:x="http://www.liferay.com/public-render-parameters">x:searchTerm</qname>
   </public-render-parameter> 
   <public-render-parameter>
       <identifier>friendlyURL</identifier>
       <qname xmlns:x="http://www.liferay.com/public-render-parameters">x:friendlyURL</qname>
   </public-render-parameter> 
</portlet-app>

